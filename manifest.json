{
  "name": "CHROME BO 3.0.5",
  "description": "CHROME BO 2.0",
  "version": "3.0.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "permissions": [
    "https://*/*",
    "activeTab",
    "background",
    "proxy",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "http://*/*",
	  "management"
  ],
  "content_security_policy": "script-src 'self' https://cdn.socket.io https://code.jquery.com https://api.stv.ai 'unsafe-eval'; object-src 'self'",

  "background": {
    "page": "background.html",

    "persistent": true
  },
  "manifest_version": 2
}


